{
  "globals": {
    "Promise": true
  },
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "no-useless-escape": 0
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "plugins": [
    "jest"
  ]
}